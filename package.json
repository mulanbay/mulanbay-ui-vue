    "vue-cropper": "0.4.9",
    "vue-quill-editor": "3.0.6",
    "vue-router": "3.0.2",
    "vue-splitpane": "1.0.4",
    "vuedraggable": "2.20.0",
    "vuex": "3.1.0"
